{
  "name": "trading-sto",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "backend:build": "cd apps/backend && cargo build",
    "backend:dev": "cd apps/backend && cargo run"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }, 
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.4",
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "@types/react": "18.3.1",
      "@types/react-dom": "18.3.1"      
    }
  }  
}
