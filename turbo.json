{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "target/**"]      
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "backend#build": {
      "outputs": ["target/**"]
    },
    "backend#dev": {
      "cache": false
    }
  }
}
